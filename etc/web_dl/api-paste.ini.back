[pipeline:web_dl]
pipeline = versionfilter apiv1app

[pipeline:web_dl_2]
pipeline = versionfilter apiv2app

[app:apiv1app]
paste.app_factory = web_dl.web_dl:Router

[app:apiv2app]
paste.app_factory = web_dl.factory_router:public_app_factory

[filter:versionfilter]
paste.filter_factory = web_dl.middleware.versionfilter:VersionFilter.factory

[composite:main]
use = egg:Paste#urlmap
/ = web_dl
/test = web_dl_2
